{
  "name": "incident_mgmt",
  "version": "0.1.0",
  "private": true,
  "license": "",
  "description": "React frontend for Incident Management",
  "dependencies": {
    "@date-io/date-fns": "1.3.13",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.2",
    "@mui/styles": "^5.2.2",
    "@reduxjs/toolkit": "1.5.0",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "@types/jest": "26.0.15",
    "@types/node": "12.0.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.11",
    "@types/react-router-dom": "5.1.7",
    "axios": "^0.19.1",
    "date-fns": "^2.14.0",
    "date-fns-tz": "^1.3.0",
    "express": "^4.17.3",
    "i18next": "20.3.2",
    "i18next-browser-languagedetector": "6.1.2",
    "moment": "^2.27.0",
    "momentjs": "^2.0.0",
    "react": "^17.0.2",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-i18next": "11.11.0",
    "react-moment": "^0.9.7",
    "react-redux": "^7.2.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "redux": "4.0.5",
    "redux-observable": "1.2.0",
    "rxjs": "6.6.3",
    "typescript": "4.1.3",
    "web-vitals": "1.0.1"
  },
  "scripts": {
    "start": "set PORT=3001 && react-scripts start",
    "build": "CI=false react-scripts build",
    "build-local": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test-ci": "cross-env CI=true react-scripts test --coverage --watchAll=false --testResultsProcessor jest-sonar-reporter"
  },
  "proxy": "http://localhost:3000",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.179",
    "cross-env": "^7.0.2",
    "husky": "4.3.8",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "10.5.3",
    "prettier": "2.1.1",
    "source-map-explorer": "2.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,css,md,ts,tsx}": [
      "prettier --write"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "src(.*)$": "<rootDir>/src$1"
    }
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
